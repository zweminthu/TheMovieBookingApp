<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="text">
        <item name="android:textSize">20sp</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">#00FF6A</item>
    </style>
    <style name="we_ve_sent_">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="please_enter">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#888888</item>
    </style>
    <style name="label">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#7A7E80</item>
    </style>

    <style name="header">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">#00FF6A</item>
        <item name="android:shadowColor">#00FF6A</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">20</item>
    </style>

    <style name="search_cities">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">#9E9E9E</item>
    </style>

    <style name="bot_nav_text_active">
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textColor">#00FF6A</item>
    </style>
    <style name="bot_nav_text_inactive">
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textColor">#888888</item>
    </style>

    <style name="food_drinks">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">Inter</item>
        <item name="lineHeight">19sp</item>
        <item name="textAllCaps">false</item>
    </style>

</resources>